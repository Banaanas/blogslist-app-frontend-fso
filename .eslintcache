te exact path=\"/users/:id\">\n          <SingleUserPage />\n        </Route>\n\n        <Route path=\"/blogs/:id\">\n          <SingleBlogPage />\n        </Route>\n\n        <Route exact path=\"/login\">\n          {isAuthenticated ? (\n            <motion.div exit=\"undefined\">\n              {/* Read the note below */}\n              <Redirect to=\"/my-profile\" />\n            </motion.div>\n          ) : (\n            <LoginPage />\n          )}\n        </Route>\n\n        <Route exact path=\"/signup\">\n          {isAuthenticated ? (\n            <motion.div exit=\"undefined\">\n              {/* Read the note below */}\n              <Redirect to=\"/my-profile\" />\n            </motion.div>\n          ) : (\n            <SignUpPage />\n          )}\n        </Route>\n\n        <PrivateRoute exact path=\"/add-blog\" component={AddBlogPage} />\n\n        <Route>\n          <NotFoundPage />\n        </Route>\n\n        {/* Framer Motion - React Router Redirect - Issue\n        Wrapping the <Redirect /> Component into a <motion /> Component\n        with exit=\"undefined\"\n        --> https://github.com/framer/motion/issues/466\n        Especially, the bug happens for private route, when, after going to the\n        AddBlogPage, you logout.*/}\n\n      </Switch>\n    </AnimatePresence>\n  );\n};\n\nexport default Routes;\n","/home/cyril/Bureau/blogslist-app-frontend-FSO/src/utils/displayToast.js",[],"/home/cyril/Bureau/blogslist-app-frontend-FSO/src/utils/userLogout.js",[],"/home/cyril/Bureau/blogslist-app-frontend-FSO/src/Components/Footer/Footer.js",[],"/home/cyril/Bureau/blogslist-app-frontend-FSO/src/utils/displayServerErrorToast.js",[],"/home/cyril/Bureau/blogslist-app-frontend-FSO/src/Components/Footer/SubFooter.js",[],"/home/cyril/Bureau/blogslist-app-frontend-FSO/src/Components/Footer/NavBar.js",[],"/home/cyril/Bureau/blogslist-app-frontend-FSO/src/Components/Footer/ContactIcons.js",[],"/home/cyril/Bureau/blogslist-app-frontend-FSO/src/routes/Privat